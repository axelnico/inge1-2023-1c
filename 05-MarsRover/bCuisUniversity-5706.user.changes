profundidadDelSueloHasta: tipoDeCapa
	
	
	^capas inject: 0 into: [:alturaAcumulada :capa |
						capa = tipoDeCapa 
							ifTrue: [^alturaAcumulada ]
							ifFalse: [
								  alturaAcumulada + capa altura
								]
						 ]
	
	! !

