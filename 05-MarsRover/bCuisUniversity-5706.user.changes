!Island methodsFor: 'as yet unclassified' stamp: 'NB 4/30/2023 15:07:40' prior: 50634087!
terrainAt: aPosition

	| ix |
	
	ix := 1.
	[ix <= treesAt size] whileTrue: [ | pos |
		pos := treesAt at: ix.
		(aPosition = pos) ifTrue: [ ^ #Tree ].		
		ix := ix + 1.
	].

	ix := 1.
	[ix <= digsAt size] whileTrue: [ | pos |
		pos := digsAt at: ix.
		(aPosition = pos) ifTrue: [ ^ #Diggable ].
		ix := ix + 1.
	].

	^ #Empty.
	
! !
